class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        
        int curIndex = 0;
        for(int i=0; i<n; i++){
            if(nums[i]!=0){
                nums[curIndex] = nums[i];
                curIndex++;
            }
        }

        for(int i=curIndex; i<n; i++){
            nums[i] = 0;
        }
    }
};
