import java.io.BufferedReader
import java.io.InputStreamReader

fun main() {
    val br = BufferedReader(InputStreamReader(System.`in`))
    val bw = System.out.bufferedWriter()

    val n = br.readLine().toInt()
    val applicants: IntArray = br.readLine().split(" ").map { it.toInt() }.toIntArray()
    val (c, d) = br.readLine().split(" ").map { it.toInt() }

    val needed = applicants.sumOf { applicant ->
        if (applicant <= c){
          1L
        } else {
            val remain = applicant - c
            1L + (remain + d - 1) / d
        }
    }

    bw.write(needed.toString())

    bw.flush()
    br.close()
}
